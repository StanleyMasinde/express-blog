{
  "name": "express-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "migrate": "knex migrate:latest",
    "pretest": "knex migrate:rollback && knex migrate:latest",
    "test": "NODE_ENV=development mocha --exit",
    "lint": "eslint --ext .js --ignore-path .gitignore ./"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~3.1.0",
    "dotenv": "^8.2.0",
    "express": "~4.17.3",
    "express-session": "^1.17.1",
    "knex": "^2.4.0",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pluralize": "^8.0.0",
    "pug": "^3.0.2",
    "sqlite3": "^5.0.3"
